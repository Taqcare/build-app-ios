{
	"appId": "com.taqcare.app",
	"appName": "Taqcare",
	"webDir": "dist",
	"server": {
		"androidScheme": "https",
		"cleartext": true
	},
	"ios": {
		"contentInset": "automatic",
		"version": "1.0.0",
		"buildNumber": "2"
	},
	"android": {
		"contentInset": "automatic"
	},
	"plugins": {
		"LocalNotifications": {
			"smallIcon": "ic_stat_icon_config_sample",
			"iconColor": "#488AFF",
			"sound": "beep.wav"
		},
		"Camera": {
			"web": {
				"showFilePicker": true
			}
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		}
	},
	"packageClassList": [
		"CAPCameraPlugin",
		"FilesystemPlugin",
		"LocalNotificationsPlugin",
		"PushNotificationsPlugin"
	]
}
